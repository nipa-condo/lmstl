{
  "name": "lmstl-core",
  "version": "1.0.9",
  "description": "mini classroom project this project is for education",
  "type": "commonjs",
  "dependencies": {
    "@google-cloud/dialogflow": "^5.5.1",
    "@google-cloud/storage": "^7.9.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.1",
    "concurrently": "^7.6.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "envalid": "^7.3.1",
    "express": "^4.18.1",
    "googleapis": "131.0.0",
    "html-to-text": "^9.0.3",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "mime": "^4.0.1",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^3.1.0",
    "pg": "^8.4.0",
    "pug": "^3.0.2",
    "redis": "^4.1.0",
    "reflect-metadata": "^0.1.13",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.20",
    "typeorm-naming-strategies": "^4.1.0",
    "zod": "^3.14.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.3.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/html-to-text": "^9.0.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/lodash": "^4.14.191",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.11.10",
    "@types/pug": "^2.0.6",
    "morgan": "^1.10.0",
    "prettier": "^3.0.3",
    "ts-node": "10.7.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "4.5.2"
  },
  "scripts": {
    "start": "ts-node-dev --respawn --transpile-only --exit-child src/index.ts",
    "start:dev": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec ts-node src/index.ts",
    "start:prod": "node dist/index",
    "build": "tsc -p .",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:create": "npm run typeorm migration:create",
    "typeorm:migrate": "npm run typeorm -- -d src/ormconfig.ts migration:generate",
    "typeorm:run": "npm run typeorm -- -d ./src/ormconfig.ts migration:run",
    "typeorm:rollback": "npm run typeorm -- -d src/ormconfig.ts migration:revert"
  },
  "license": "UNLICENSED"
}
